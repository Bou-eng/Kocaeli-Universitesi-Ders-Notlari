	self.buton1.setGeometry(QtCore.QRect(510, 555,270, 35)) #1 enine 2 boyuna kordinat 3.sü genişlik 4.cüsü boy


	QtCore.QObject.connect(self.buton,QtCore.SIGNAL("clicked()"),self.islem)
	QtCore.QObject.connect(self.buton1,QtCore.SIGNAL("clicked()"),self.isle)

	lab = QtGui.QLabel(Dialog)
	res1 = QtGui.QPixmap("logo.png")
	lab.setPixmap(res1 )
	lab.setGeometry(QtCore.QRect(180,-1, 521, 60))
	self.retranslateUi(Dialog)
	QtCore.QMetaObject.connectSlotsByName(Dialog)
 
