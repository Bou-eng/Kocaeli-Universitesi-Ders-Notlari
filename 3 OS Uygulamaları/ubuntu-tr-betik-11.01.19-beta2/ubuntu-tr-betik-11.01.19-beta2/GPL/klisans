    def islem(self):
	dosya = open(".kdurum","w")
	dosya.write("Hayır")
	dosya.close()
	Dialog.destroy()
    def isle(self):
	dosya = open(".kdurum","w")
	dosya.write("Evet")
	dosya.close()
	Dialog.destroy()
